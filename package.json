{
  "name": "chat_app_api",
  "version": "0.0.0",
  "description": "api for a chat application",
  "main": "main.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon",
    "prod": "cross-env NODE_ENV=production nodemon",
    "test:unit": "jest --watch --coverage",
    "lint:fix": "eslint . --ext .js,.ts --fix --ignore-path .gitignore",
    "build": "rimraf ./build && tsc",
    "start:production": "npm run build && node build/index.js",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "openapi-generate": "openapi  --useOptions --client axios --input ./specs/contrat.yaml --output ./src/services/chat"
  },
  "devDependencies": {
    "@types/bcrypt": "~5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-session": "~2.0.44",
    "@types/express": "^4.17.15",
    "@types/jest": "~29.2.2",
    "@types/jsonwebtoken": "~9.0.0",
    "@types/node": "^16.18.12",
    "@types/validator": "^13.7.12",
    "@wibrc/eslint-config-vue": "github:IT-WIBRC/wibrc-eslint-config-vue",
    "cross-env": "^7.0.3",
    "dotenv": "~16.0.3",
    "husky": "~8.0.2",
    "jest": "~29.3.1",
    "lint-staged": "12.1.2",
    "nodemon": "~2.0.20",
    "openapi-typescript-codegen": "~0.23.0",
    "rimraf": "~3.0.2",
    "sequelize-cli": "^6.5.2",
    "supertest": "~6.3.1",
    "ts-jest": "~29.0.3",
    "ts-node": "~10.9.1",
    "tslint": "^6.1.3",
    "typescript": "~4.8.4",
    "uuid": "~9.0.0"
  },
  "dependencies": {
    "axios": "^1.2.1",
    "bcrypt": "~5.1.0",
    "compression": "^1.7.4",
    "cookie-session": "~2.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "helmet": "^6.0.1",
    "jsonwebtoken": "~9.0.0",
    "mysql2": "^2.3.3",
    "sequelize": "~6.25.5",
    "sequelize-typescript": "^2.1.5",
    "socket.io": "^4.5.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IT-WIBRC/Chat_App_API.git"
  },
  "keywords": [
    "api",
    "chat",
    "app"
  ],
  "author": "wibrc",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/IT-WIBRC/Chat_App_API/issues"
  },
  "homepage": "https://github.com/IT-WIBRC/Chat_App_API#readme"
}
